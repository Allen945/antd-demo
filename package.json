{
  "name": "antd-demo",
  "version": "1.0.0",
  "description": "Just a demo.",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rm -rf dist && rm -rf build/*",
    "build": "npm run babel && npm run webpack",
    "babel": "./node_modules/.bin/babel src -d dist --experimental --copy-files",
    "webpack": "./node_modules/.bin/webpack --config ./config/webpack.config.js -p --progress --colors",
    "start": "npm run babel-w & npm run dev-webpack & npm run server",
    "babel-w": "./node_modules/.bin/babel src -d dist --experimental --watch --copy-files",
    "dev-webpack": "node webpack-dev-server.js",
    "server": "NODE_ENV=development nodemon dist/server.js"
  },
  "author": "Brick Yang <brick.c.yang@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "antd": "^2.8.1",
    "mobx": "^3.1.7",
    "moment": "^2.17.1",
    "nunjucks": "^3.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-import": "^1.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "nodemon": "^1.11.0",
    "postcss": "^5.2.16",
    "postcss-loader": "^1.3.3",
    "postcss-modules-values": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "react-hot-loader": "^1.3.1",
    "style-loader": "^0.14.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
}
